<% provide(:title,  @user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for @user %>
        <%= @user.name %>
        <%= @user.dice_point %><br>
        <% @point_logs.each do |point_logs| %>
          <%= point_logs.service_name %>
          <%= point_logs.category %>
          <%= point_logs.dice_point %><br>
        <% end %>

              </h1>
    </section>
  </aside>
</div>